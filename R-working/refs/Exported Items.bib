@article{fonseca_carbon_2012,
	title = {Carbon accumulation in aboveground and belowground biomass and soil of different age native forest plantations in the humid tropical lowlands of Costa Rica},
	volume = {43},
	issn = {0169-4286},
	url = {http://www.springerlink.com/content/km57220t311r1530/abstract/},
	doi = {10.1007/s11056-011-9273-9},
	abstract = {Generic or default values to account for biomass and carbon accumulation in tropical forest ecosystems are generally recognized as a major source of errors, making site and species specific data the best way to achieve precise and reliable estimates. The objective of our study was to determine carbon in various components (leaves, branches, stems, structural roots and soil) of single-species plantations of Vochysia guatemalensis and Hieronyma alchorneoides from 0 to 16 years of age. Carbon fraction in the biomass, mean (±standard deviation), for the different pools varied between 38.5 and 49.7\% (±3 and 3.8). Accumulated carbon in the biomass increased with the plantation age, with mean annual increments of 7.1 and 5.3 Mg ha −1 year −1 for forest plantations of V. guatemalensis and H. alchorneoides , respectively. At all ages, 66.3\% (±10.6) of total biomass was found within the aboveground tree components, while 18.6\% (±20.9) was found in structural roots. The soil (0–30 cm) contained 62.2 (±13) and 71.5\% (±17.1) of the total carbon (biomass plus soil) under V. guatemalensis and H. alchorneoides , respectively. Mean annual increment for carbon in the soil was 1.7 and 1.3 Mg ha −1 year −1 in V. guatemalensis and H. alchorneoides . Allometric equations were constructed to estimate total biomass and carbon in the biomass which had an R 2 aj (adjusted R square) greater than 94.5\%. Finally, we compare our results to those that could have resulted from the use of default values, showing how site and species specific data contribute to the overall goal of improving carbon estimates and providing a more reliable account of the mitigation potential of forestry activities on climate change.},
	number = {2},
	urldate = {2012-06-06},
	journal = {New Forests},
	author = {Fonseca, William and Alice, Federico and Rey-Benayas, José},
	year = {2012},
	keywords = {Biomedical and Life Sciences},
	pages = {197--211},
	file = {Fonseca et al. - 2012 - Carbon accumulation in aboveground and belowground.pdf:/Users/dfalster/Zotero/storage/QKBA7I75/Fonseca et al. - 2012 - Carbon accumulation in aboveground and belowground.pdf:application/pdf;SpringerLink Snapshot:/Users/dfalster/Zotero/storage/4D285QN4/Fonseca et al. - 2012 - Carbon accumulation in aboveground and belowground.html:text/html}
},

