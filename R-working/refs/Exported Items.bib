	author = {Osada, Noriyuki},
	month = dec,
	year = {2005},
	keywords = {Allometry, {ANACARDIACEAE}, {BRANCHING} (Botany), crown architecture, {FOREST} biomass, individual leaf area, leaf area index, onset of branching, {PLANT} allometry, {PLANT} species, {RHUS}, {SAPINDALES}, {STELE} (Botany), tree height, {TREES} -- Growth},
	pages = {1590--1598},
